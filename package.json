{
  "name": "lifebalancer-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "REACT_APP_UPDATED_AT=$(date +%FT%T%z) react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "lt --port 3000 --subdomain lifebalancer",
    "codegen": "graphql-codegen",
    "deploy": "npm run build && netlify deploy --prod",
    "deploy:beta": "REACT_APP_HOST=https://lifebalancer-beta.netlify.app npm run build && npm run netlify:deploy:beta",
    "netlify:deploy:beta": "NETLIFY_SITE_ID=d6e3f97a-d488-4601-b2fb-463a7fdb54c1 netlify deploy --prod",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "icons:gen": "sh ./scripts/generateIcons/generateIcons.sh",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "icons:cap:android": "npx capacitor-resources --platforms android --icon ./resources/android/icon.png --splash ./resources/android/splash.png && cap copy android",
    "icons:cap:ios": "npx capacitor-resources --platforms ios --icon ./resources/ios/icon.png --splash ./resources/ios/splash.png && cap copy ios"
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@auth0/auth0-react": "^1.3.0",
    "@auth0/auth0-spa-js": "^1.13.1",
    "@capacitor/android": "^2.4.7",
    "@capacitor/core": "^2.4.7",
    "@capacitor/ios": "^2.4.7",
    "@date-io/luxon": "^1.3.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "@iconify-icons/ic": "^1.1.3",
    "@iconify/react": "^1.1.4",
    "@ionic-native/core": "^5.32.1",
    "@ionic-native/device": "^5.32.1",
    "@ionic-native/in-app-purchase-2": "^5.32.1",
    "@ionic-native/mobile-accessibility": "^5.32.1",
    "@ionic-native/network": "^5.32.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "@react-navigation/bottom-tabs": "^5.11.7",
    "@react-navigation/native": "^5.9.2",
    "@react-navigation/stack": "^5.14.2",
    "@sentry/react": "^6.1.0",
    "@sentry/tracing": "^6.1.0",
    "ably": "^1.2.4",
    "apollo3-cache-persist": "^0.9.1",
    "bson-objectid": "^1.3.1",
    "clsx": "^1.1.1",
    "console-feed": "^3.2.1",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-network-information": "^2.0.2",
    "cordova-plugin-purchase": "^10.5.3",
    "devextreme": "^20.2.5",
    "devextreme-react": "^20.2.5",
    "emoji-mart": "^3.0.0",
    "eslint-plugin-unused-imports": "^1.1.0",
    "firebase": "^7.24.0",
    "gradient-color": "^2.0.1",
    "graphql": "^15.4.0",
    "history": "^4.10.1",
    "lodash": "^4.17.20",
    "logrocket": "^1.0.14",
    "luxon": "^1.25.0",
    "material-table": "1.62.0",
    "phonegap-plugin-mobile-accessibility": "^1.0.5",
    "pluralize": "^8.0.0",
    "polished": "^4.0.5",
    "query-string": "^6.13.1",
    "react": "^17.0.1",
    "react-cool-onclickoutside": "^1.5.9",
    "react-device-detect": "^1.15.0",
    "react-dom": "^17.0.1",
    "react-infinite-scroll-component": "^6.0.0",
    "react-loadable": "^5.5.0",
    "react-native-paper": "^4.7.1",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^2.17.1",
    "react-native-vector-icons": "^8.0.0",
    "react-native-web": "^0.15.0",
    "react-page-visibility": "^6.2.0",
    "react-responsive": "^8.2.0",
    "react-router-dom": "^5.2.0",
    "react-scroll-restoration": "^1.0.6",
    "react-spinners": "^0.10.4",
    "react-swipeable": "^5.5.1",
    "react-textfit": "^1.1.1",
    "react-transition-group": "^4.4.1",
    "react-use": "^15.3.4",
    "remeda": "0.0.22",
    "scroll-behavior-polyfill": "^2.0.13",
    "styled-components": "^5.2.1",
    "subscriptions-transport-ws": "^0.9.18",
    "use-debounce": "^5.2.0",
    "use-long-press": "^1.0.4",
    "use-persisted-state": "^0.3.3",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^2.4.7",
    "@graphql-codegen/cli": "^1.19.2",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-operations": "^1.17.10",
    "@graphql-codegen/typescript-react-apollo": "^2.1.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/d3-scale": "^3.2.2",
    "@types/emoji-mart": "^3.0.4",
    "@types/history": "^4.7.8",
    "@types/jest": "^24.0.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.161",
    "@types/luxon": "^1.25.0",
    "@types/node": "^12.0.0",
    "@types/pluralize": "0.0.29",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-loadable": "^5.5.4",
    "@types/react-native": "^0.63.49",
    "@types/react-responsive": "^8.0.2",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.3",
    "@types/use-persisted-state": "^0.3.0",
    "@types/uuid": "^8.3.0",
    "capacitor-resources": "^2.0.5",
    "cli-real-favicon": "0.0.8",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "netlify-cli": "^2.71.0",
    "prettier": "^2.1.2",
    "pwa-asset-generator": "^4.1.0",
    "react-scripts": "^4.0.2",
    "serve": "^11.3.2",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.1.5",
    "workbox-plugin-firebase-auth": "^1.1.0",
    "workbox-window": "^5.1.4"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "unused-imports"
    ],
    "rules": {
      "no-unused-vars": "off",
      "unused-imports/no-unused-imports": "warn",
      "unused-imports/no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "varsIgnorePattern": "^_",
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "npx prettier --write",
      "eslint --fix --max-warnings=0"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
